{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "consistent-return": "off",
      "import/newline-after-import": "off",
      "max-len": "off"
    }
}
